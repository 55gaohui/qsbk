.common-list{
	padding: 20rpx;
}
.common-list-l{
	-webkit-flex-shrink: 0;
	    -ms-flex-negative: 0;
	        flex-shrink: 0;
}
.common-list-l image{
	width: 90rpx;
	height: 90rpx;
	-webkit-border-radius:100%;
	        border-radius:100%;
}
.common-list-r{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	margin-left: 15rpx;
	border-bottom: 1rpx solid #EEEEEE;
	padding-bottom: 10rpx;
}
.common-list-r>view:nth-child(3)>image{
	width: 100%;
	-webkit-border-radius:10rpx;
	        border-radius:10rpx;
}
.common-list-r>view:nth-child(1)>view:first-child{
	color: #999999;
	font-size: 32rpx;
}
.common-list-r>view:nth-child(1)>view:last-child{
	background: #EEEEEE;
	padding: 0 10rpx;
	font-size: 26rpx;
}
.common-list-r>view:nth-child(2){
	font-size: 32rpx;
	padding: 12rpx 0;
}
.common-list-r>view:nth-child(3){
	position: relative;
	margin-bottom: 10rpx;
}
.common-list-play,.common-list-playinfo{
	position: absolute;
	color: #FFFFFF;
}
.common-list-play{
	font-size: 130rpx;
}
.common-list-playinfo{
	right: 10rpx;
	bottom: 10rpx;
	background: rgba(51, 51, 51, 0.73);
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
	padding: 0 20rpx;
	font-size: 26rpx;
}
.common-list-r>view:nth-child(4)>view{
	color: #AAAAAA;
}
.common-list-r>view:nth-child(4)>view:nth-child(2)>view{
	margin-left: 10rpx;
	padding-left: 5rpx;
	font-size: 28rpx;
}
.common-list-share{
	background: #EEEEEE;
	width: 100%;
	padding: 10rpx;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
}
.common-list-share>image{
	width: 200rpx;
	height: 150rpx;
	margin-right: 10rpx;
}

